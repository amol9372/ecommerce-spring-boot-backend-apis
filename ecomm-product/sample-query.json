{
  "bool": {
    "filter": [
      {
        "terms": {
          "features.Brand": [
            "Samsung",
            "Apple"
          ]
        }
      }
    ],
    "must": [
      {
        "match": {
          "name": {
            "_name": "Galaxy",
            "query": "Galaxy"
          }
        }
      }
    ]
  }
}